@model IEnumerable<User>

<div class="container">
    <table class='table table-striped'>
        <tr>
            <th>UserId</th>
            <th>Username</th>
            <!--<th>FirstName</th>-->
            <!--<th>LastName</th>-->
            <th>Email</th>
            <th>Password</th>
            <th>Pins</th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.Password</td>
                <td>
                    @if (@user.Pins != null)
                    {
                        <p>@string.Join(", ", @user.Pins.Select(pin => $"Pin{pin.PinId}"))</p>
                    }
                </td>
            </tr>
        }
    </table>
</div>