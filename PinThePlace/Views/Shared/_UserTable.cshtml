@model IEnumerable<User>

<div class="container">
    <table class="table table-striped">
        <tr>
            <th>UserId</th>
            <th>UserName</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>

        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.UserName</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>
                </td>
                <td></td>
            </tr>
        }

    </table>
</div>