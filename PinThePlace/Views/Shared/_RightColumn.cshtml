@model PinsViewModel

<div class="col-md-6">
            <!--lenker til feed og mypins-->
            <div>
                <a id="showFeedWindow" href="#">Feed window</a>
                <a id="showFavWindow" href="#">Favorites window</a>
            </div>
            <!--Feed vindu-->
            <div id="feedwindow" class="contentwindow">
                <h2>Feed</h2>
                <partial name="_PinTable" model="Model.Pins"/>
            </div>
            <!--mypins vindu-->
            <div id="favwindow" class="contentwindow" style="display: none;">
                <h2>My Pins <3 </h2>
                <partial name="_MyPins" model="Model.Pins"/>
            </div>
</div>

<script src="~/js/jquery.min.js"></script>
<script src="~/js/bootstrap.min.js"></script>
<script>
    // javascript kode for Ã¥ kunne bytte mellom vinduer
$(document).ready(function(){
    $("#showFeedWindow").click(function(e){
        e.preventDefault();
        $("#feedwindow").show();
        $("#favwindow").hide();
    });

    $("#showFavWindow").click(function(e){
        e.preventDefault();
        $("#favwindow").show();
        $("#feedwindow").hide();
    });
});

</script>